<!DOCTYPE html>
<html lang="en">
    <!-- Generated by Cligen @ <%= Time.new.strftime("%Y-%m-%d %H:%M:%S") %> -->
    <head>
        <meta charset="utf-8">
        <title><%= @data["title"].capitalize %></title>
        <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.1/css/bootstrap.min.css">
        <style>
            body {
                padding-top: 80px;
                text-align: center;
            }

            a {
                font-size: 200%;
            }
        </style>
    </head>
    <body>
        <div class="container">
            <h1><%= @data["title"].capitalize %></h1>

            <p class="lead">Put something here.</p>
            <div>
                <% @data["pages"].each do |key, value| %>
                    <a
                        href="<%= value %>"
                        class="btn btn-lg btn-<%= @data["button_styles"].sample %>"
                    >
                        <%= key.capitalize %>
                    </a>
                <% end %>
            </div>
        </div>
    </body>
</html>
